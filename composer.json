{
  "name": "metamodels/attribute_file",
  "description": "MetaModels attribute for files",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "file",
    "nontranslatable"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/attribute_file/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_file"
  },
  "require": {
    "php": ">=5.6",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "contao-community-alliance/composer-plugin": "~3.0",
    "metamodels/core": "~2.1@dev",
    "symfony/dependency-injection": "^3.3",
    "symfony/http-kernel": "^3.3"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "phpcq/all-tasks": "~1.1",
    "metamodels/base-unit-tests": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\Attribute\\File\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\Test\\Attribute\\File\\": "tests"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "src/Resources/contao": "system/modules/metamodelsattribute_file",
        "src/Resources/public": "assets/metamodelsattribute_file"
      },
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_file-",
        "languages_cto": "contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
